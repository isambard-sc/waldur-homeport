{
  "name": "waldur-homeport",
  "version": "4.8.6",
  "description": "Web interface for Waldur MasterMind cloud orchestrator",
  "repository": "https://github.com/waldur/waldur-homeport",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "postinstall": "husky",
    "start": "VITE_API_URL=http://localhost:8000/ vite --port 8001",
    "build": "NODE_OPTIONS=--max-old-space-size=32768 vite build",
    "preview": "vite preview",
    "format:check": "prettier --check \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.css\" \"src/**/*.scss\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.css\" \"src/**/*.scss\"",
    "style:check": "stylelint \"src/**/*.css\" \"src/**/*.scss\"",
    "lint:check": "eslint --ext ts,tsx src",
    "lint:fix": "eslint --fix --ext ts,tsx src",
    "deps:unused": "knip --include files,types,enumMembers",
    "deps:circular": "madge --exclude broadcasts/BroadcastFooter.tsx --circular src/**/*",
    "test": "yarn build",
    "ci:start": "VITE_API_URL=http://localhost:8001/ yarn build && yarn serve dist --single --listen 8001",
    "ci:run": "cypress run",
    "ci:test": "start-server-and-test ci:start http://localhost:8001/ ci:run",
    "gettext:extract": "node extractLiteralsFromFiles.cjs"
  },
  "dependencies": {
    "@cloudfour/simple-svg-placeholder": "^1.1.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@fontsource/inter": "^4.5.14",
    "@fontsource/open-sans": "^4.5.14",
    "@jonkoops/matomo-tracker": "^0.7.0",
    "@phosphor-icons/react": "^2.1.5",
    "@popperjs/core": "~2.10.1",
    "@redux-saga/delay-p": "^1.2.1",
    "@sentry/react": "^7.30.0",
    "@sentry/tracing": "^7.30.0",
    "@tanstack/react-query": "^4.26.1",
    "@uirouter/react": "^1.0.7",
    "attr-accept": "^1.1.0",
    "axios": "^1.6.7",
    "bootstrap": "5.1.3",
    "cidr-regex": "^3.1.1",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.3.1",
    "d3-scale-chromatic": "^2.0.0",
    "dompurify": "^2.2.6",
    "draft-js": "^0.11.4",
    "draftjs-to-html": "^0.9.1",
    "echarts": "^4.8.0",
    "file-saver": "^1.3.3",
    "flatpickr": "^4.6.13",
    "font-awesome": "~4.7.0",
    "html-to-draftjs": "^1.5.0",
    "immutable": "^3.8.2",
    "ip-regex": "^4.2.0",
    "jszip": "^3.7.1",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.11.1",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "markdown-to-jsx": "^7.3.2",
    "monaco-editor": "^0.52.0",
    "papaparse": "^5.2.0",
    "pdfmake": "^0.2.2",
    "prop-types": "^15.6.2",
    "qs": "^6.9.3",
    "react": "^18.3.0",
    "react-bootstrap": "^2.10.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^18.3.0",
    "react-draft-wysiwyg": "^1.14.7",
    "react-dropzone": "^14.2.3",
    "react-flatpickr": "^3.10.13",
    "react-leaflet": "^4.2.1",
    "react-rating-stars-component": "^2.2.0",
    "react-redux": "^8.0.0",
    "react-responsive": "^10.0.0",
    "react-select": "^5.8.0",
    "react-select-async-paginate": "^0.7.4",
    "react-ultimate-pagination": "^1.3.2",
    "react-use": "^17.5.0",
    "react-window": "^1.8.10",
    "react-window-paginated": "^1.0.0",
    "react-windowed-select": "^5.2.0",
    "reapop": "^4.2.2",
    "redux": "^4.1.0",
    "redux-form": "^8.3.10",
    "redux-form-saga": "^0.2.2",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "text-security": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/helper-plugin-utils": "^7.25.9",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/plugin-syntax-typescript": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/classnames": "^2.2.3",
    "@types/cypress": "^1.1.3",
    "@types/dompurify": "^2.0.4",
    "@types/jest": "^27.0.2",
    "@types/leaflet": "^1.5.19",
    "@types/leaflet-geosearch": "^2.7.1",
    "@types/lodash": "^4.14.21",
    "@types/luxon": "^2.0.4",
    "@types/node": "^20.14.6",
    "@types/qs": "^6.9.5",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-flatpickr": "^3.8.6",
    "@types/react-loadable": "^5.5.4",
    "@types/react-redux": "^7.1.4",
    "@types/react-ultimate-pagination": "^1.2.1",
    "@types/react-window": "^1.8.8",
    "@types/redux-form": "^8.3.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.3.7",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "cypress": "^13.6.5",
    "cypress-recurse": "^1.35.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.1",
    "jest": "^29.7.0",
    "jest-date-mock": "^1.0.5",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^13.0.0",
    "knip": "^5.2.2",
    "lint-staged": "^10.0.8",
    "madge": "^8.0.0",
    "mock-match-media": "^0.4.3",
    "oxlint": "^0.10.3",
    "postcss": "^8.4.38",
    "postcss-import": "^14.0.2",
    "prettier": "^3.2.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "sass": "^1.43.4",
    "serve": "^14.2.4",
    "start-server-and-test": "^2.0.8",
    "stylelint": "^16.3.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "ts-import-plugin": "^3.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.3",
    "vite": "^5.4.10",
    "vite-plugin-markdown": "^2.2.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "lint-staged": {
    "**/*.js": [
      "yarn prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "yarn eslint --ext ts,tsx --fix"
    ],
    "**/*.{css,scss}": [
      "yarn prettier --write"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(tsx?|jsx?)$": [
        "ts-jest",
        {
          "diagnostics": {
            "ignoreCodes": [
              151001
            ]
          }
        }
      ],
      "^.+\\.svg$": "<rootDir>/test/svgTransform.js",
      "^.+\\.png$": "<rootDir>/test/pngTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!axios/.*)"
    ],
    "testRegex": "\\.spec\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "@waldur/(.*)": "<rootDir>/src/$1",
      "\\.(css|scss|svg)$": "<rootDir>/test/style-mock.js"
    },
    "setupFiles": [
      "<rootDir>/test/react-setup.js",
      "jest-date-mock",
      "mock-match-media/jest-setup"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/cypress/"
    ],
    "coverageReporters": [
      "cobertura"
    ],
    "testEnvironment": "jsdom",
    "reporters": [
      "default",
      "jest-junit"
    ],
    "snapshotFormat": {
      "escapeString": true,
      "printBasicPrototype": true
    }
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "suiteNameTemplate": "{filepath}"
  },
  "knip": {
    "cypress": {
      "config": "cypress.config.js",
      "entry": [
        "cypress/e2e/**/*.ts",
        "cypress/support/e2e.ts",
        "cypress/support/commands.ts"
      ]
    },
    "eslint": {
      "entry": [
        ".eslintrc.cjs",
        "cypress/.eslintrc.cjs"
      ]
    },
    "ignore": [
      "extractLiteralsFromFiles.cjs",
      "extractLiteralsFromFiles.ts"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-recommended-scss",
    "ignoreFiles": [
      "./src/metronic/**/*.scss"
    ],
    "rules": {
      "no-descending-specificity": null,
      "scss/load-no-partial-leading-underscore": null
    }
  },
  "jscpd": {
    "ignore": [
      "src/form/BoxNumberField.scss",
      "src/metronic/sass/core/components/buttons/_theme.scss",
      "src/metronic/sass/core/base/functions/_get.scss"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "autoprefixer": {},
      "cssnano": {
        "zindex": false
      }
    }
  }
}
