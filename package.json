{
  "name": "waldur-homeport",
  "version": "4.8.6",
  "description": "Web interface for Waldur MasterMind cloud orchestrator",
  "repository": "https://github.com/waldur/waldur-homeport",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "postinstall": "husky",
    "start": "VITE_API_URL=http://localhost:8000/ vite --port 8001",
    "build": "NODE_OPTIONS=--max-old-space-size=32768 vite build",
    "preview": "vite preview",
    "format:check": "prettier --check \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.css\" \"src/**/*.scss\"",
    "format:fix": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.css\" \"src/**/*.scss\"",
    "style:check": "stylelint \"src/**/*.css\" \"src/**/*.scss\"",
    "lint:check": "eslint src",
    "lint:fix": "eslint --fix src",
    "deps:unused": "knip --include files,types,enumMembers,exports",
    "deps:circular": "madge --exclude broadcasts/BroadcastFooter.tsx --circular src/**/*",
    "test": "vitest",
    "ci:start": "VITE_API_URL=http://localhost:8001/ yarn build && yarn serve dist --single --listen 8001",
    "ci:run": "cypress run",
    "ci:test": "start-server-and-test ci:start http://localhost:8001/ ci:run",
    "gettext:extract": "node extractLiteralsFromFiles.cjs"
  },
  "dependencies": {
    "@cloudfour/simple-svg-placeholder": "^1.1.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@fontsource/inter": "^4.5.14",
    "@fontsource/open-sans": "^4.5.14",
    "@jonkoops/matomo-tracker": "^0.7.0",
    "@phosphor-icons/react": "^2.1.5",
    "@popperjs/core": "~2.10.1",
    "@sentry/react": "^8.38.0",
    "@tanstack/react-query": "^4.26.1",
    "@uirouter/react": "^1.0.7",
    "attr-accept": "^1.1.0",
    "axios": "1.7.8",
    "bootstrap": "5.1.3",
    "cidr-regex": "^3.1.1",
    "classnames": "^2.2.5",
    "d3-scale-chromatic": "^2.0.0",
    "dompurify": "^3.1.7",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "echarts": "^5.5.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "final-form": "^4.20.10",
    "final-form-arrays": "^3.1.0",
    "flatpickr": "^4.6.13",
    "html-to-draftjs": "^1.5.0",
    "immutable": "^3.8.2",
    "ip-regex": "^4.2.0",
    "jszip": "3.8.0",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^3.11.1",
    "lodash-es": "^4.17.21",
    "luxon": "3.2.1",
    "markdown-to-jsx": "7.4.0",
    "monaco-editor": "^0.52.0",
    "papaparse": "^5.2.0",
    "pdfmake": "^0.2.17",
    "prop-types": "^15.6.2",
    "qs": "^6.9.7",
    "react": "^18.3.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.3.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-dropzone": "^14.2.3",
    "react-final-form": "^6.5.9",
    "react-final-form-arrays": "^3.1.4",
    "react-flatpickr": "^3.10.13",
    "react-leaflet": "^4.2.1",
    "react-rating-stars-component": "^2.2.0",
    "react-redux": "^8.0.0",
    "react-responsive": "^10.0.0",
    "react-select": "^5.8.0",
    "react-select-async-paginate": "^0.7.4",
    "react-ultimate-pagination": "^1.3.2",
    "react-use": "^17.6.0",
    "react-window": "^1.8.10",
    "react-window-paginated": "^1.0.0",
    "react-windowed-select": "^5.2.0",
    "reapop": "^4.2.2",
    "redux": "^4.1.0",
    "redux-form": "^8.3.10",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "text-security": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/helper-plugin-utils": "^7.25.9",
    "@babel/plugin-syntax-jsx": "^7.25.9",
    "@babel/plugin-syntax-typescript": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/classnames": "^2.2.3",
    "@types/cypress": "^1.1.3",
    "@types/dompurify": "^3.0.5",
    "@types/leaflet": "^1.5.19",
    "@types/leaflet-geosearch": "^2.7.1",
    "@types/lodash-es": "^4.17.12",
    "@types/luxon": "^2.0.4",
    "@types/node": "^20.14.6",
    "@types/qs": "^6.9.5",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-flatpickr": "^3.8.6",
    "@types/react-loadable": "^5.5.4",
    "@types/react-redux": "^7.1.4",
    "@types/react-ultimate-pagination": "^1.2.1",
    "@types/react-window": "^1.8.8",
    "@types/redux-form": "^8.3.10",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.3.7",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "cypress": "^13.16.0",
    "cypress-recurse": "^1.35.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "husky": "^9.0.1",
    "jsdom": "^25.0.1",
    "knip": "^5.36.7",
    "lint-staged": "^10.0.8",
    "madge": "^8.0.0",
    "mock-match-media": "^0.4.3",
    "oxlint": "^0.13.1",
    "postcss": "^8.4.49",
    "postcss-import": "^14.0.2",
    "prettier": "^3.3.3",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "sass": "^1.81.0",
    "serve": "^14.2.4",
    "start-server-and-test": "^2.0.8",
    "stylelint": "^16.12.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "ts-import-plugin": "^3.0.0",
    "typescript": "^5.4.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.9",
    "vite-plugin-markdown": "^2.2.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.4",
    "vitest-location-mock": "^1.0.4"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "lint-staged": {
    "**/*.js": [
      "yarn prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "yarn eslint --fix"
    ],
    "**/*.{css,scss}": [
      "yarn prettier --write"
    ]
  },
  "knip": {
    "cypress": {
      "config": "cypress.config.js",
      "entry": [
        "cypress/e2e/**/*.ts",
        "cypress/support/e2e.ts",
        "cypress/support/commands.ts"
      ]
    },
    "eslint": {
      "entry": [
        "eslint.config.js",
        "cypress/.eslintrc.cjs"
      ]
    },
    "ignore": [
      "extractLiteralsFromFiles.cjs",
      "extractLiteralsFromFiles.ts",
      "*.spec.ts",
      "*.spec.tsx",
      "*.fixture.ts",
      "*.fixture.tsx",
      "src/EventsEnums.ts",
      "src/FeaturesEnums.ts",
      "src/core/draftjs-module.tsx"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-recommended-scss",
    "ignoreFiles": [
      "./src/metronic/**/*.scss"
    ],
    "rules": {
      "no-descending-specificity": null,
      "scss/load-no-partial-leading-underscore": null
    }
  },
  "jscpd": {
    "ignore": [
      "src/form/BoxNumberField.scss",
      "src/metronic/sass/core/components/buttons/_theme.scss",
      "src/metronic/sass/core/base/functions/_get.scss"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "autoprefixer": {},
      "cssnano": {
        "zindex": false
      }
    }
  }
}
